<?xml version="1.0" encoding="iso-8859-1"?>
<idea-plugin>
    <extensions defaultExtensionNs="org.jetbrains.bsp">
        <bspProjectOpenProcessorExtension
                implementation="org.jetbrains.plugins.scala.bsp.flow.open.ScalaBspProjectOpenProcessorExtension"/>
        <bspProjectAwareExtension
                implementation="org.jetbrains.plugins.scala.bsp.config.ScalaBspProjectAwareExtension"/>
        <buildTargetClassifierExtension
                implementation="org.jetbrains.plugins.scala.bsp.extension.ScalaBuildTargetClassifier"/>
        <buildToolAssetsExtension
                implementation="org.jetbrains.plugins.scala.bsp.assets.ScalaAssetsExtension"/>
        <scalaSdkExtension
                implementation="org.jetbrains.plugins.scala.project.bsp.ScalaSdkExtensionImpl"/>
        <connectionDetailsProviderExtension
                implementation="org.jetbrains.plugins.scala.bsp.server.connection.ScalaConnectionDetailsProvider"/>
    </extensions>
    <extensions defaultExtensionNs="com.intellij">
        <projectOpenProcessor
                implementation="org.jetbrains.plugins.scala.bsp.flow.open.ScalaBspProjectOpenProcessor"/>
    </extensions>
</idea-plugin>
