class a {
  def foo() =
    b match {
      case _: ScIfStmt | _: ScWhileStmt | _: ScDoStmt
      | _: ScFinallyBlock | _: ScCatchBlock => {
        child.getPsi match {
          case _: ScExpression => Indent.getNormalIndent
          case _ => Indent.getNoneIndent
        }
      }
    }
}
